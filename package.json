{
  "name": "angular-cli-app-with-rollup",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ivy-ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc-app-watch": "ngc -w -p src/tsconfig.app.json",
    "ngc-app": "ngc -p src/tsconfig.app.json",
    "rollup-clean-dev": "rimraf out-tsc && rimraf dist-rollup",
    "rollup-clean-prod": "rimraf out-tsc && rimraf dist-rollup-prod",
    "rollup-watch": "rollup -c -w --environment BUILD:dev",
    "rollup-dev": "npm run rollup-clean-dev && npm run ngc-app && npm run rollup-dev-app",
    "rollup-dev-app": "npm-run-all --parallel ngc-app-watch rollup-watch",
    "rollup-prod": "npm run rollup-clean-prod && npm run ngc-app && rollup -c --environment BUILD:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-next.2",
    "@angular/common": "9.0.0-next.2",
    "@angular/compiler": "9.0.0-next.2",
    "@angular/core": "9.0.0-next.2",
    "@angular/forms": "9.0.0-next.2",
    "@angular/platform-browser": "9.0.0-next.2",
    "@angular/router": "9.0.0-next.2",
    "core-js": "~2.6.5",
    "rxjs": "~6.5.2",
    "systemjs": "~5.0.0",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.1",
    "@angular/cli": "8.2.1",
    "@angular/compiler-cli": "9.0.0-next.2",
    "@angular/language-service": "9.0.0-next.2",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "browser-sync": "^2.26.7",
    "codelyzer": "~5.0.1",
    "compression": "^1.7.3",
    "fs-extra": "^8.1.0",
    "http2": "^3.3.7",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "npm-run-all": "~4.1.5",
    "protractor": "~5.4.2",
    "rollup": "~1.16.6",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-commonjs": "~10.0.1",
    "rollup-plugin-entrypoint-hashmanifest": "^0.1.2",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-sourcemaps": "~0.4.2",
    "rollup-plugin-static-site": "~0.1.0",
    "rollup-plugin-terser": "~5.1.1",
    "strip-json-comments": "^2.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  }
}
