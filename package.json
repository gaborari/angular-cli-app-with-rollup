{
  "name": "angular-cli-app-with-rollup",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ivy-ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc-app-watch": "ngc -w -p src/tsconfig.app.json",
    "ngc-app": "ngc -p src/tsconfig.app.json",
    "rollup-clean-dev": "rimraf out-tsc && rimraf dist-rollup",
    "rollup-clean-prod": "rimraf out-tsc && rimraf dist-rollup-prod",
    "rollup-watch": "rollup -c -w --environment BUILD:dev",
    "rollup-dev": "npm run rollup-clean-dev && npm run ngc-app && npm run rollup-dev-app",
    "rollup-dev-app": "npm-run-all --parallel ngc-app-watch rollup-watch",
    "rollup-prod": "npm run rollup-clean-prod && npm run ngc-app && rollup -c --environment BUILD:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0-beta.11",
    "@angular/common": "8.0.0-beta.11",
    "@angular/compiler": "8.0.0-beta.11",
    "@angular/core": "8.0.0-beta.11",
    "@angular/forms": "8.0.0-beta.11",
    "@angular/platform-browser": "8.0.0-beta.11",
    "@angular/router": "8.0.0-beta.11",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "systemjs": "^3.0.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "8.0.0-beta.11",
    "@angular/language-service": "8.0.0-beta.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rollup": "^1.0.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-static-site": "^0.0.3",
    "browser-sync": "^2.26.3",
    "http2": "^3.3.7",
    "compression": "^1.7.3",
    "strip-json-comments": "^2.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
